{
    "name": "klimick/doctrine-php-mapping",
    "description": "Doctrine mapping using php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Klimenko Andrew (klimick)",
            "email": "klimichkartorgnusov@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Klimick\\DoctrinePhpMapping\\": ["./src"],
            "Klimick\\PsalmDoctrinePhpMapping\\": ["./psalm"]
        },
        "files": [
            "src/Dsl/Embed.php",
            "src/Dsl/Field.php",
            "src/Dsl/Id.php",
            "src/Dsl/InverseSide.php",
            "src/Dsl/JoinColumn.php",
            "src/Dsl/JoinedTableInheritance.php",
            "src/Dsl/ManyToMany.php",
            "src/Dsl/ManyToOne.php",
            "src/Dsl/OneToOne.php",
            "src/Dsl/OwningSide.php",
            "src/Dsl/SingleTableInheritance.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Klimick\\DoctrinePhpMapping\\Test\\": ["./test"]
        }
    },
    "require": {
        "php": "^8.0",
        "doctrine/persistence": "^2.2",
        "doctrine/orm": "^2.9",
        "whsv26/functional": "^2.0"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "vimeo/psalm": "^4.8",
        "klimick/psalm-show-type": "^1.0",
        "psr/cache": "2",
        "symfony/cache": "^5.3"
    }
}
